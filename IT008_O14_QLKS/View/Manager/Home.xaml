                        </Border>
                    </Grid>
                </Border>
                <Border Margin="10,45,10,148" BorderBrush="White" Background="#FFE9E5D9" OpacityMask="White" CornerRadius="5,5,5,5" >
                    <Grid>
                        <Ellipse HorizontalAlignment="Left" Height="99" Stroke="#FFC6980A" VerticalAlignment="Top" Width="99" Margin="183,103,0,0">
                            <Ellipse.Fill>
                                <ImageBrush x:Name="avt" ImageSource="/user.png" />
                            </Ellipse.Fill>
                        </Ellipse>

                        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="29" Margin="19,16,0,0" VerticalAlignment="Top" Width="259" CornerRadius="5,5,5,5">
                            <Grid Margin="52,0,0,0">
                                <Label Content="Name" HorizontalAlignment="Left" VerticalAlignment="Center" FontFamily="Sylfaen " FontWeight="Bold" FontSize="16" Margin="-49,0,0,0"/>
                                <Label Content="" HorizontalAlignment="Left" VerticalAlignment="Center" FontFamily="Sylfaen " FontWeight="Bold" FontSize="16" Margin="1,0,0,0" Width="199"/>
                            </Grid>
                        </Border>
                        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="29" Margin="19,59,0,0" VerticalAlignment="Top" Width="259" CornerRadius="5,5,5,5">
