            <RowDefinition Height="79*"/>
            <RowDefinition Height="511*"/>
        </Grid.RowDefinitions>
        <Border BorderBrush="Black" BorderThickness="1" Margin="0,0,355,0" CornerRadius="5,5,5,5" Grid.RowSpan="2">
            <Border.Background>
                <ImageBrush ImageSource="/View/Manager/home_2.png"/>
            </Border.Background>
        </Border>
