                                <Label Content="" HorizontalAlignment="Left" VerticalAlignment="Center" FontFamily="Sylfaen " FontWeight="Bold" FontSize="16" Margin="39,0,0,0" Width="162"/>

                            </Grid>
                        </Border>
                        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="29" Margin="19,0,0,0" VerticalAlignment="Center" Width="140" CornerRadius="5,5,5,5">
                            <Grid Margin="52,0,0,0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="25*"/>
                                    <RowDefinition Height="2*"/>
                                </Grid.RowDefinitions>
                                <Label Content="Gender" HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="Sylfaen " FontWeight="Bold" FontSize="16" Margin="-49,-3,0,0" Width="61" Height="36" Grid.RowSpan="2"/>
                                <Label Content="" HorizontalAlignment="Left" VerticalAlignment="Center" FontFamily="Sylfaen " FontWeight="Bold" FontSize="16" Margin="12,0,0,0" Width="75"/>

                            </Grid>
                        </Border>
                        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="29" Margin="19,145,0,0" VerticalAlignment="Top" Width="140" CornerRadius="5,5,5,5">
                            <Grid Margin="52,0,0,0">
                                <Label Content="Age" HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="Sylfaen " FontWeight="Bold" FontSize="16" Margin="-50,-2,0,0" Height="36"/>
                                <Label Content="" HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="Sylfaen " FontWeight="Bold" FontSize="16" Margin="-15,-1,0,0" Height="36" Width="99"/>

                            </Grid>
                        </Border>
                        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="29" Margin="19,186,0,0" VerticalAlignment="Top" Width="140" CornerRadius="5,5,5,5">
                            <Grid Margin="52,0,0,0">
                                <Label Content="Phone" HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="Sylfaen " FontWeight="Bold" FontSize="16" Margin="-49,-2,0,0"/>
                                <Label Content="" HorizontalAlignment="Center" VerticalAlignment="Center" FontFamily="Sylfaen " FontWeight="Bold" FontSize="16" Width="82"/>

                            </Grid>
                        </Border>
