        <Border x:Name="Diagram1" BorderBrush="Black" BorderThickness="1" Margin="10,18,367,337" Background="#FFC6980A" CornerRadius="5,5,5,5" Grid.Row="1">
            <Label Content="Diagram#1" HorizontalAlignment="Left" Height="36" Margin="14,1,0,0" VerticalAlignment="Top" Width="150"  FontSize="26" FontFamily="Sylfaen " Foreground="White" Background="#00000000" />
        </Border>
        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="73" Margin="12,13,0,0" VerticalAlignment="Top" Width="403" CornerRadius="5,5,5,5" Background="#FFF3F3F3" Grid.RowSpan="2">
            <Grid Margin="0,-3,1,3">
                <Label Content="Activity" HorizontalAlignment="Left" Height="47" Margin="14,-5,0,0" VerticalAlignment="Top" Width="150"  FontSize="26" FontFamily="Sylfaen " Foreground="Black" Background="#00000000" />
                <Label Content="October 21th, 2023" HorizontalAlignment="Left" Height="47" Margin="14,25,0,0" VerticalAlignment="Top" Width="241"  FontSize="18" FontFamily="Sylfaen " Foreground="#FFCDA529" Background="#00000000" />
                <Border Margin="342,17,17,13" Background="#FFC6980A" CornerRadius="5,5,5,5" >
                    <Image Source="/View/Manager/icons8-calender-32 1.jpg" Margin="4,3,4,4"/>
                </Border>
            </Grid>
        </Border>
        <Ellipse x:Name="avt_load" HorizontalAlignment="Left" Height="56" Margin="683,13,0,0" Stroke="#00000000" VerticalAlignment="Top" Width="56" MouseDown="avt_load_MouseDown" Cursor="Hand">
            <Ellipse.Fill>
                <ImageBrush x:Name="avtt" ImageSource="/icons8-upload-64.png" />
            </Ellipse.Fill>
        </Ellipse>
        <Label Content="NDHunq" HorizontalAlignment="Left" Height="48" Margin="483,2,0,0" VerticalAlignment="Top" Width="150"  FontSize="26" FontFamily="Sylfaen " Foreground="Black" Background="#00000000" />
