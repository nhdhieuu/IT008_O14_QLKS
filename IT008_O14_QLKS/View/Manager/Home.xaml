                    </Grid>
                </Border>
            </Grid>
        </Border>
        <Border x:Name="Diagram2" BorderBrush="Black" BorderThickness="1" Margin="12,187,367,12" Background="#FF0AC6C6" CornerRadius="5,5,5,5" Grid.Row="1">
