                    <Grid>
                        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="29" Margin="19,16,0,0" VerticalAlignment="Top" Width="259" CornerRadius="5,5,5,5">
                            <Grid Margin="52,0,0,0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="2*"/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <Label Content="Position" HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="Sylfaen " FontWeight="Bold" Margin="-49,0,0,0" FontSize="16" Grid.RowSpan="2"/>
                                <Label Content="" HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="Sylfaen " FontWeight="Bold" FontSize="16" Margin="1,0,0,0" Width="199" Grid.RowSpan="2"/>
                            </Grid>
                        </Border>
                        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="29" Margin="19,59,0,0" VerticalAlignment="Top" Width="259" CornerRadius="5,5,5,5">
                            <Grid Margin="52,0,0,0">
                                <Label Content="ID" HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="Sylfaen " FontWeight="Bold" FontSize="16" Margin="-49,-3,0,0"/>
                                <Label Content="" HorizontalAlignment="Left" VerticalAlignment="Center" FontFamily="Sylfaen " FontWeight="Bold" FontSize="16" Margin="1,0,0,0" Width="199"/>
                            </Grid>
                        </Border>
                        <Border x:Name="Change_pass" BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="24" Margin="158,101,0,0" VerticalAlignment="Top" Width="109" CornerRadius="5,5,5,5" Background="#FFC6980A" MouseDown="Change_pass_MouseDown" MouseEnter="Change_pass_MouseEnter" MouseLeave="Change_pass_MouseLeave" Cursor="Hand">

